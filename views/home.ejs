<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <title>BLOG</title>
    <link rel="stylesheet" href="./stylesheets/style.css" type="text/css" />
    <script src="/send_url.js"></script>
</head>

<body>
    <div class="top">
        <div class="wrapper">
            <div class="content">
                <% data.forEach((post) => {%>
                <% const { title, body, published } = post %>
                <h2 class="text-amber-700"><%= title %></h2>
                <% }) %>

                <h2>Login</h2>
                <form action="/" method="post">
                    <p class="text-amber-700">Title</p>
                    <input type="text" name="title">
                    <p>Body</p>
                    <input type="text" name="body">
                    <input type="submit" value="Send">
                </form>
            </div>
        </div>
</body>

</html>